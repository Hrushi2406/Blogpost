
<%- include("partials/header"); -%>

<h1>Home</h1>
<p><%= StartingContent %></p>

<% posts.forEach(function(post){ %>
<h1><%= post.title %></h1>
<p><%= post.content.substring(0, 100) + "..." %><a href="/post/<%= post.title %>">Read More</a></p>
<% }) %>

<form  action="/home" method="post">
  <button class="btn btn-primary comp"  type="submit" name="button">Compose</button>
</form>


<%- include("partials/footer"); -%>
